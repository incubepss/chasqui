{
  "name": "chasqui-vendure",
  "version": "2.0.4",
  "private": true,
  "license": "GPL-3.0-only",
  "scripts": {
    "compile-admin-ui": "ts-node src/compile-admin-ui.ts",
    "copy:assets": "copyfiles -u 1 ./src/**/*.json ./src/**/*.scss ./src/images/**/* ./src/run-prod.sh ./dist",
    "dev-plugins": "ts-node-dev ./src/index.dev.ts --recompile false",
    "dev:server": "ts-node ./src/index.dev.ts",
    "dev-watch:worker": "ts-node-dev ./src/index-worker.dev.ts",
    "dev:worker": "ts-node ./src/index-worker.dev.ts",
    "dev": "concurrently yarn:dev:* --env",
    "test": "yarn jest",
    "coverage": "yarn jest --coverage",
    "run:server": "ts-node ./src/index.ts",
    "run:worker": "ts-node ./src/index-worker.ts",
    "start": "concurrently yarn:run:* --env",
    "build": "rimraf dist && rimraf admin-ui && tsc --build tsconfig.prod.json && yarn compile-admin-ui && yarn copy:assets",
    "migration:generate": "ts-node ./src/migration generate",
    "migration:run": "ts-node ./src/migration run",
    "migration:revert": "ts-node ./src/migration revert",
    "generate-types": "graphql-codegen --config ./codegen.yml",
    "lint": "eslint ."
  },
  "dependencies": {
    "@asymmetrik/ngx-leaflet": "^13.0.2",
    "@asymmetrik/ngx-leaflet-draw": "^13.0.3",
    "@nestjs/schedule": "^1.0.2",
    "@vendure/admin-ui-plugin": "1.7.3",
    "@vendure/asset-server-plugin": "1.7.3",
    "@vendure/core": "1.7.3",
    "@vendure/email-plugin": "1.7.3",
    "@vendure/harden-plugin": "1.9.3",
    "@vendure/job-queue-plugin": "1.7.3",
    "axios": "^0.24.0",
    "body-parser": "^1.20.2",
    "bullmq": "^1.78.1",
    "commander": "^8.3.0",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "leaflet": "1.8.0",
    "leaflet-draw": "1.0.2",
    "leaflet-draw-locales": "^1.2.1",
    "mercadopago": "^1.5.8",
    "odoo-await": "^3.4.1",
    "pg": "8.7.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.13.2",
    "@graphql-codegen/fragment-matcher": "^1.17.8",
    "@graphql-codegen/schema-ast": "^2.2.0",
    "@graphql-codegen/typescript": "^1.13.2",
    "@graphql-codegen/typescript-compatibility": "^2.1.5",
    "@graphql-codegen/typescript-operations": "^1.13.2",
    "@types/cron": "^1.7.3",
    "@types/jest": "^27.0.3",
    "@types/leaflet": "1.7.9",
    "@types/leaflet-draw": "1.0.2",
    "@types/mercadopago": "^1.5.5",
    "@types/node": "^16.11.13",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@vendure/testing": "1.7.3",
    "@vendure/ui-devkit": "1.7.3",
    "concurrently": "6.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.3.5"
  },
  "resolutions": {
    "@vendure/admin-ui": "1.7.3",
    "mjml": "4.13.0"
  }
}
